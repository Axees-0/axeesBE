# Frontend Bug Hunter Environment Configuration
# Copy this file to .env and configure for your environment

# Frontend Application URL
# Local Development (Expo): http://localhost:19006
# Staging: https://staging.axees.com
# Production: https://app.axees.com
FRONTEND_URL=http://localhost:19006

# Test Configuration
# Duration for different test types (in seconds)
QUICK_TEST_TIMEOUT=300
STANDARD_TEST_TIMEOUT=900
COMPREHENSIVE_TEST_TIMEOUT=1800

# Browser Configuration
HEADLESS=true
BROWSER_WIDTH=1920
BROWSER_HEIGHT=1080

# Mobile Testing Configuration
MOBILE_WIDTH=375
MOBILE_HEIGHT=667

# Performance Thresholds
PAGE_LOAD_TIMEOUT=5000
NETWORK_TIMEOUT=30000

# Debug Configuration
DEBUG_MODE=false
SCREENSHOT_ON_FAILURE=true
VERBOSE_LOGGING=false

# CI/CD Configuration
CI_MODE=false
PARALLEL_TESTS=2

# Authentication Test Data (for staging/test environments only)
TEST_EMAIL=test@axees.com
TEST_PASSWORD=test123
TEST_PHONE=+1234567890